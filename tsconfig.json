{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.js",
    "expo-env.d.js",
    "constants/sample_data.js",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/cases.jsx",
    "app/(tabs)/documents.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/profile.jsx",
    "app/(tabs)/tasks.jsx",
    "app/auth/login.jsx",
    "app/_layout.jsx",
    "app/index.jsx",
    "components/active-task.jsx",
    "components/all-case.jsx",
    "components/completed-task.jsx",
    "components/create-task.jsx",
    "components/protected-route.jsx",
    "context/auth-context.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
