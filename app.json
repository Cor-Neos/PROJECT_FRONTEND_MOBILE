{
  "expo": {
    "name": "Legal Vault",
    "slug": "legal_vault_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dark_logo.jpg",
    "scheme": "legalvaultmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "linking": {
      "prefixes": ["legalvaultmobile://"]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {}
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "API_BASE_URL": "http://192.168.1.11:3000/api"
    }
  }
}
